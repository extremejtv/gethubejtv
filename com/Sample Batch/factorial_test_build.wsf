<job id="JSUtility">

<reference object="Scripting.FileSystemObject" />

<script language="JScript" src="JSUtility_BatchCodeC.js" />
<script language="JScript">
//
//	Define files, options, and operations to be performed 
//
// Use the following syntax to include any  
// JavaScript files you wish to combine as a single entity
// for JSUtility to  process
InputFiles.push("factorial.js");
InputFiles.push("factorial_test.js");

// Set the options for the Javascript Utility
// using the following syntax for each option (one or more options on each line). 
Obfuscate.Options={DefinedGlobalVars:true, ObjectMembers:true, StringVariables:true, MinimumStringLength:12,RestructureIfStmts:true};
Compact.Options={AddMissingSemicolons:true, LeaveNewLines:false, LeaveNewLineAndComment:false, RemoveUnnecessarySemicolons:true};
Compress.Options={ISO8859Compatable:true, UTF8Compatible:false};

// If desired, enter comments to be included in the beginning of the output file in variable OutputPreamble.
OutputPreamble="//Factorial_TestC written by Patrick J. O'Neil \n//Please use as a sample for your own code.\n\n";

// If desired, select the destination of the output on successful completion.
OutputFile="Factorial_TestC.js";
ReportFile="Factorial_Test_Report.html";

// Select the JSUtility operations you want to run and 
// whether to stop on validation warning messages
Operations={Validate:true, Obfuscate:true, Compact:true, Compress:true, StopOnWarning:false};

// Now execute the batch operations
JSUtilityBatch();

</script>
</job>

